  "shopKeeperData": {
    "welcomeWords": [
      {
        "id": "CN_000",
        "text": "ハロードクター！今日は何探してる？"
      }
    ],
    "clickWords": [
      {
        "id": "CN_001",
        "text": "このクロージャの購買部に置いてないものはないよー。ゆっくり見てってー！"
      },
      {
        "id": "CN_002",
        "text": "ヒント：お買い得な商品ばかりだよー。"
      },
      {
        "id": "CN_003",
        "text": "何買うって？全部でしょっ！"
      },
      {
        "id": "CN_004",
        "text": "仕入れ品リストに入ってるものならなんでも注文を受け付けるよ！"
      },
      {
        "id": "CN_005",
        "text": "この世で一番の悲しみ、それはキミを持ち帰れないこと…。"
      },
      {
        "id": "CN_006",
        "text": "もう出血大サービスしてるから、これ以上安くって言われても無理だって！ほんとだってば！"
      },
      {
        "id": "CN_007",
        "text": "ドクターが買わないなら、あたしがもらっちゃうよ。"
      }
    ]
  },
